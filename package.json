    "mermaid": "10.8.0",
    "object.groupby": "^1.0.1",
    "ts-results": "3.3.0",
    "tslib": "2.6.2",
    "yaml": "2.3.4"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@types/mock-fs": "4.13.4",
    "@types/node": "20.11.15",
    "@types/npmcli__map-workspaces": "3.0.4",
    "@types/object.groupby": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@vitest/coverage-v8": "1.2.2",
    "eslint": "8.56.0",
    "mock-fs": "5.2.0",
    "package-json-type": "^1.0.3",
    "prettier": "3.2.4",
    "publint": "0.2.7",
    "ts-pattern": "5.0.6",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "vitest": "1.2.2"
